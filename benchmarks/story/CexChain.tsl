guarantee {
    // Verify the following system architecture
    // -> [1] -> [2] -> [3] ->

    // Input simulation
    G [in <- f in];

    // Compund spec
    G F [mem1 <- in];
    G F [out1 <- mem1];
    G F [mem2 <- out1];
    G F [out2 <- mem2];
    G F [mem3 <- out2];
    G F [out <- mem3];

    // Specification
    ! G (p in -> F (p out));

}
